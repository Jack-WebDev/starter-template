{
	"name": "Fullstack Turbo Dev",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/app",
	"customizations": {
		"vscode": {
			"extensions": ["biomejs.biome"],
			"settings": {
				"terminal.integrated.defaultProfile.linux": "bash",
				"biome.enable": true,
				"biome.lint.enable": true,
				"biome.format.enable": true
			}
		}
	},
	"remoteUser": "node"
}
